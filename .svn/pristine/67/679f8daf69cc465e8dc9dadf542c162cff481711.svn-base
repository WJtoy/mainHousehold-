<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wolfking.jeesite.ms.tmall.rpt.dao.B2BOrderDailyQtyDao">
    <sql id="rptWorkcardDailyQty">
        a.yearmonth,
        a.d1, a.d2, a.d3, a.d4, a.d5, a.d6, a.d7, a.d8, a.d9, a.d10,
        a.d11, a.d12, a.d13, a, a.d15, a.d16, a.d17, a.d18, a.d19, a.d20,
        a.d21, a.d22, a.d23, a.d24, a.d25, a.d26, a.d27, a.d28, a.d29, a.d30, a.d31

    </sql>
<select id="getB2BOrderDailyQty" resultType="com.kkl.kklplus.entity.b2b.order.B2BWorkcardQtyDaily">
     SELECT *
     FROM b2b_order_qty_daily
     WHERE yearmonth = #{yearmonth}
     <if test="dataSource!=null">
         AND data_source = #{dataSource}
     </if>
    <if test="statisticType!=null">
        AND statistic_type = #{statisticType}
    </if>
    ORDER BY data_source
    LIMIT 200000
</select>
</mapper>
