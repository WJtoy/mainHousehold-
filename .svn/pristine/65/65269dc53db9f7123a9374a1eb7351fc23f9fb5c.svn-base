package com.wolfking.jeesite.modules.sys.entity;


import com.google.common.collect.Lists;

import java.util.List;

public class UserRegionViewModel {

    private Long userId;

    private Integer subFlag;

    public List<UserRegion> getRegionList() {
        return regionList;
    }
    public Integer getSubFlag() {
        return subFlag;
    }

    public void setSubFlag(Integer subFlag) {
        this.subFlag = subFlag;
    }

    public Long getUserId() {
        return userId;
    }

    public void setUserId(Long userId) {
        this.userId = userId;
    }

    private List<UserRegion> regionList = Lists.newArrayList();



    public void setRegionList(List<UserRegion> regionList) {
        this.regionList = regionList;
    }
}
