syntax="proto3";

//option java_package = "com.wolfking.jeesite.modules.mq.dto";
option java_outer_classname = "MQCreateOrderPushMessage";

import "Customer.proto";

package com.wolfking.jeesite.modules.mq.dto;
//下单后推送消息内容
//1.订单信息
//2.报表
message CreateOrderPushMessage {
    int64 id = 1;
    //订单ID
    int64 orderId = 2;
    //订单号
    string orderNo = 3;
    //分片
    string quarter = 4;
    //类型
    string type = 5;
    //触发者
    TriggerBy triggerBy = 6;
    //触发时间
    int64 triggerDate = 7;
    //
    int32 orderApproveFlag = 8;
    //OrderFee
    OrderFee orderFee = 9;
    //area
    int64 areaId = 10;
    //kefu
    TriggerBy kefu = 11;
    //push message
    string msgContent = 12;
    //customer
    com.wolfking.jeesite.modules.mq.dto.Customer customer = 13;
    //user phone
    string userPhone = 14;
    //category id
    int64 categoryId = 15;
}

//用户
message TriggerBy{
    //ID
    int64 id = 1;
    //姓名
    string name = 2;
}

//Fee
message OrderFee{
    //预估服务费
    double expectCharge = 1;
    //产品冻结费用
    double blockedCharge = 2;
    //厂商加急费
    double customerUrgentCharge = 3;
    //网点加急费
    double engineerUrgentCharge = 4;
    //厂商付款方式
    int32 orderPaymentType = 5;
    //厂商付款方式名称
    string orderPaymentTypeName = 6;
}
