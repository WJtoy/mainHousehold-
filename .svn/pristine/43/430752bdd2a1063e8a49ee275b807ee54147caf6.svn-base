	<update id="updateDetailInvoiceDate">
		UPDATE
			sd_orderdetail
		SET
			engineer_invoice_date = #{engineerInvoiceDate}
		WHERE
			<if test="quarter != null and quarter != ''.toString()">quarter = #{quarter} and</if>
			id = #{id}
	</update>

	<update id="updateDetailInvoiceDateByOrderId">
		UPDATE
			sd_orderdetail
		SET
			engineer_invoice_date = #{engineerInvoiceDate}
		WHERE
			<if test="quarter != null and quarter != ''.toString()">quarter = #{quarter} and</if>
			order_id = #{orderId}
			AND del_flag = 0
	</update>

	<update id="updateChargeFlag" parameterType="OrderCondition">
		UPDATE
			sd_order_condition
		SET
			charge_flag = #{chargeFlag},
			auto_charge_flag = #{autoChargeFlag},
			status = CONVERT(#{status.value},SIGNED),
			sub_status = #{subStatus}
		WHERE
			<if test="quarter != null and quarter != ''.toString()">quarter = #{quarter} and</if>
			order_id = #{orderId}
	</update>

	<update id="updateStatusFlagsFromCharge" parameterType="OrderStatus">
