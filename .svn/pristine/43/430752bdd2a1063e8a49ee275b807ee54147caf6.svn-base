			<if test="actionCodeId != null">
                action_code_id = #{actionCodeId},
            </if>
			<if test="actionCodeName != null">
                action_name = #{actionCodeName},
            </if>
			<if test="otherActionRemark != null">
				other_action_remark = #{otherActionRemark},
            </if>
			<if test="syncChargeTags != null">
				sync_charge_tags = #{syncChargeTags},
			</if>
		</set>
		where
			id = #{id}
		    <if test="quarter != null and quarter != ''.toString()"> and quarter = #{quarter}</if>
	</update>

	<!-- 查询符合对帐条件的订单id -->
	<select id="findIdListForCharge" resultType="java.util.HashMap">
		SELECT
				o.order_id,o.status,o.charge_flag,o.quarter,o.close_date,DATE_FORMAT(o.close_date,'%Y-%m-%d'),sof.engineer_payment_type,DATE_FORMAT(o.close_date,'%H-%i-%s')
		FROM
				sd_order_condition o
		INNER JOIN
				sd_order_fee sof
			ON 	o.order_id = sof.order_id
		WHERE
				o.charge_flag = 0
			AND	o.status = 80
			AND o.pending_flag > 1
			<![CDATA[ AND o.auto_charge_flag <> 3 ]]>
			<if test="customer != null and customer.id != null and customer.id >0">
				AND o.customer_id = #{customer.id}
			</if>
			<if test="productCategoryId != null and productCategoryId > 0">
				AND o.product_category_id  = #{productCategoryId}
			</if>
			<if test="productIds != ''.toString()">
				AND o.product_ids LIKE CONCAT('%,',#{productIds},',%')
			</if>
			<if test="beginDate != null">
				AND o.close_date >= #{beginDate}
			</if>
			<if test="endDate != null">
				AND #{endDate} >= o.close_date
			</if>
			<if test="orderNo != null and orderNo != ''.toString()">
				<choose>
					<when test="orderNoSearchType==1">
						AND o.order_no = #{orderNo}
					</when>
					<when test="orderNoSearchType==2">
						AND o.order_no LIKE concat(#{orderNo}, '%')
					</when>
					<otherwise>
						AND o.order_no LIKE concat('%',#{orderNo}, '%')
					</otherwise>
				</choose>
			</if>
			<if test="totalQty > 0">
				AND o.total_qty = #{productQty}
			</if>
			<if test="serviceTimes > 0">
				AND o.service_times = #{serviceTimes}
			</if>
			<if test="servicePoint != null and servicePoint.id != null">
				AND o.servicepoint_id = #{servicePoint.id}
			</if>
			<if test="travelChargeFlag > 0">
				AND EXISTS(
					SELECT order_id FROM sd_order_fee sof
					WHERE o.order_id = sof.order_id AND sof.engineer_travel_charge>0)
			</if>
			<if test="partsFlag > 0">
				AND EXISTS(
					SELECT order_id FROM sd_order_fee sof
					WHERE o.order_id = sof.order_id AND sof.engineer_material_charge>0)
			</if>
			<if test="otherChargeFlag > 0">
				AND EXISTS(
					SELECT order_id FROM sd_order_fee sof
					WHERE o.order_id = sof.order_id AND sof.engineer_other_charge>0)
			</if>
		ORDER BY DATE_FORMAT(o.close_date,'%Y-%m-%d'), sof.engineer_payment_type DESC, DATE_FORMAT(o.close_date,'%H-%i-%s')
	</select>

    <select id = "findConditionFeeForCharge" resultType="OrderChargeViewModel">
        SELECT
                soc.order_id,
                soc.status,
                soc.pending_flag,
                soc.quarter,
                soc.order_no,
                soc.customer_id,
                soc.kefu_id,
                soc.service_times,
                soc.close_date,
                soc.parts_flag,
                soc.finish_photo_qty,
                sof.service_charge,
                sof.express_charge,
                sof.travel_charge,
                sof.material_charge,
				sof.praise_fee,
                sof.other_charge,
                sof.customer_time_liness_charge,
                sof.customer_urgent_charge,
                sof.order_charge,
                sof.engineer_service_charge,
                sof.engineer_express_charge,
                sof.engineer_travel_charge,
                sof.engineer_material_charge,
                sof.engineer_other_charge,
                sof.time_liness_charge,
                sof.subsidy_time_liness_charge,
                sof.engineer_urgent_charge,
                sof.insurance_charge,
				sof.engineer_praise_fee,
                sof.engineer_total_charge
        FROM
                sd_order_condition soc
                INNER JOIN sd_order_fee sof ON soc.order_id = sof.order_id AND sof.engineer_payment_type = #{servicePoint.paymentType.value}
                INNER JOIN sd_order_status_flag sosf ON soc.order_id = sosf.order_id AND sosf.complete_status = 1
        WHERE
				soc.charge_flag = 0
            AND	soc.status = 80
            AND soc.pending_flag > 1
            <![CDATA[ AND soc.auto_charge_flag <> 3 ]]>
			<choose>
				<when test="orderNoSearchType == 1" >
					<if test="quarter != null and quarter != ''.toString()">
						AND soc.quarter = #{quarter}
					</if>
					AND soc.order_no = #{orderNo}
				</when>
				<otherwise>
					AND soc.close_date BETWEEN #{beginDate} AND #{endDate}
					<if test="customer != null and customer.id != null and customer.id > 0">
						AND soc.customer_id = #{customer.id}
					</if>
					<if test="productCategoryId != null and productCategoryId > 0">
						AND soc.product_category_id  = #{productCategoryId}
					</if>
					<if test="serviceTimes > 0">
						AND soc.service_times = #{serviceTimes}
					</if>
					<if test="servicePoint != null and servicePoint.id != null">
						AND soc.servicepoint_id = #{servicePoint.id}
					</if>
					<if test="travelChargeFlag > 0">
						AND sof.engineer_travel_charge > 0
					</if>
					<if test="partsFlag > 0">
						AND sof.engineer_material_charge > 0
					</if>
					<if test="otherChargeFlag > 0">
						AND sof.engineer_other_charge > 0
					</if>
				</otherwise>
			</choose>
        ORDER BY soc.close_date
    </select>

	<select id = "findDetailForCharge" resultType="OrderDetailChargeViewModel">
		SELECT
				sod.id,
				sod.order_id,
				sod.service_times,
				sod.service_type_id,
				sod.qty,
				sod.product_id,
				sod.product_spec,
