	<!-- 客评 -->
	<insert id="insertOrderGrade">
		insert into sd_ordergrade
		(
			quarter,
			order_id,
			order_no,
			servicepoint_id,
			engineer_id,
			grade_id,
			grade_name,
			grade_item_id,
			grade_item_name,
			sort,
			point,
			create_by,
			create_date
		)
		values (
			#{quarter},
			#{orderId},
			#{orderNo},
			#{servicePoint.id},
			#{engineer.id},
			#{gradeId},
			#{gradeName},
			#{gradeItemId},
			#{gradeItemName},
			#{sort},
			#{point},
			#{createBy.id},
			#{createDate}
		)
	</insert>
	<!-- 物理删除 -->
	<delete id="deleteOrderGrade">
		delete from sd_ordergrade
		where
			<if test="quarter != null and quarter != ''.toString()"> quarter = #{quarter} and </if>
			order_id = #{orderId}
	</delete>

	<!-- 查询待客评项目 -->
	<select id="getToOrderGrade" resultMap="OrderGradeResult">
		select
			g.id as grade_id,g.name as grade_name,g.sort,g.dict_type,g.point,g.remarks,
			i.id as item_id,i.point as item_point ,i.remarks as item_remarks,i.dict_value
		from md_grade g
		inner join md_grade_item i
		on g.id = i.md_grade_id and i.del_flag = 0 and g.del_flag = 0
		order by g.sort,i.point desc
	</select>

	<!-- 查询订单已评价项目 -->
	<select id="getOrderGradeByOrderId" resultMap="OrderGradeResult">
		select
		g.grade_id,g.order_id,g.order_no,g.grade_name,g.sort,g.point,g.grade_item_id,g.grade_item_name,g.engineer_id,g.servicepoint_id,
		grade_item_id as item_id,
		'' as dict_type,'' as dict_value
		from sd_ordergrade g
		where g.order_id= #{orderId}
            <if test="quarter != null and quarter != ''.toString()">
                g.quarter = #{quarter} and
            </if>
		order by g.sort,g.grade_id desc
	</select>

	<!-- 用户回复短信时，按用户电话获得订单id -->
	<select id="getToGradeOrdersByPhone" resultMap="OrderToGradeResult">
		select o.order_id,o.order_no,o.status as status_value,o.quarter,o.grade_flag,o.app_abnormaly_flag,o.customer_id,o.area_id,o.kefu_id,o.sub_status
		from sd_order_condition o
		where
		<if test="quarters != null and quarters.size > 0">
			o.quarter in
			<foreach collection="quarters" item="quarter" index="index" open="(" close=")" separator=",">
				${quarter}
			</foreach>
			and
		</if>
		o.service_phone = #{phone}
		order by o.order_id desc
		limit 10
	</select>

	<!-- 客评 END -->

	<!-- 数据处理 -->
	<!-- 重新设置订单统计信息，产品id,类别id,服务类型id,套组 -->
	<select id="findIdsResetProduct" resultType="java.util.HashMap">
		SELECT o.order_id,o.status,o.charge_flag,o.quarter
		from sd_order_condition o
		WHERE
			o.product_ids = ''
		order by o.order_id
		limit 20000
	</select>

