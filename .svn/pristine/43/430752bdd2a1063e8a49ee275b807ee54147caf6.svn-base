	<!-- 保存订单操作日志 -->
	<insert id="insertProcessLog" keyProperty="id" useGeneratedKeys="true">
		insert into sd_orderprocesslog (
		    <if test="id != null">id,</if>
			quarter,
			order_id,
			action,
			action_comment,
			status,
			status_value,
			status_flag,
			close_flag,
			visibility_flag,
			create_by,
			create_date
		) VALUES (
          <if test="id != null">#{id},</if>
			#{quarter},
			#{orderId},
			#{action},
			#{actionComment},
			#{status},
			#{statusValue},
			#{statusFlag},
			#{closeFlag},
			#{visibilityFlag},
			#{createBy.id},
			#{createDate}
		)
