			#{customer.id},
			#{customerType},
			#{userName},
			#{phone1},
			#{phone2},
			#{phone3},
			#{address},
			#{serviceAddress},
			#{servicePhone},
			#{status.value},
			#{kefu.id},
			#{createBy.id},
			#{createDate},
			#{createBy.id},
			#{createDate},
			#{productIds},
			#{productCategoryId},
			#{serviceTypes},
			#{hasSet},
			#{totalQty},
			#{orderServiceType},
			#{pendingTypeDate},
		    #{canRush},
		    #{kefuType},
