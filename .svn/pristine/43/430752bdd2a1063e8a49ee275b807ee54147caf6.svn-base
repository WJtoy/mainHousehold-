	<select id="findOrderListOfCrush" resultMap="CrushPlanOrderMap">
		SELECT
			c.quarter,
			c.order_id,
			c.order_no,
			c.status,
			c.user_name,
			c.service_phone,
			c.service_address,
			c.area_id,
			c.area_name,
			c.sub_area_id,
			c.servicepoint_id,
			c.create_date,
			c.product_category_id,
			o.orderitem_json
		FROM sd_order_condition c
		inner join sd_order_head o on c.order_id = o.id
		<where>
			<choose>
				<when test="areaLevel == 2 and area.id != null and area.id >0">
					and c.area_id = #{area.id}
				</when>
				<when test="areaLevel == 3 ">
					<if test="area.parent != null and area.parent.id != null and area.parent.id > 0">
						and c.area_id = #{area.parent.id}
					</if>
					AND c.sub_area_id = #{area.id}
				</when>
				<otherwise>
					and c.order_id = 0
				</otherwise>
			</choose>
			<if test="quarters != null and quarters.size > 0">
				and c.quarter in
				<foreach collection="quarters" item="quarter" index="index" open="(" close=")" separator=",">
					${quarter}
				</foreach>
			</if>
			<if test="beginDate != null and endDate != null">
				and c.create_date between #{beginDate} and #{endDate}
			</if>
			and c.servicepoint_id > 0
		</where>
		order by c.order_id desc
	</select>

