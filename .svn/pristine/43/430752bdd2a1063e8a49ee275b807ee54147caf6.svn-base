	<!-- 读取订单日志 -->
	<select id="getOrderLogsByFlags" resultType="OrderProcessLog">
		select l.id,l.action,l.action_comment,l.status,l.status_flag,l.close_flag,l.create_by as "createBy.id", '' as "createBy.name",l.create_date,l.visibility_flag
		from sd_orderprocesslog l
		where
		<if test="quarter != null and quarter != ''.toString()"> l.quarter = #{quarter} and </if>
		l.order_id = #{orderId}
		<if test="closeFlag != null">
			and l.close_flag = #{closeFlag}
		</if>
		<if test="statusFlags != null and statusFlags.size > 0">
			<choose>
				<when test="statusFlags.size == 1">
					and l.status_flag =
					<foreach collection="statusFlags" item="statusFlag" index="index" open="" close="" separator="">
						#{statusFlag}
					</foreach>
				</when>
				<otherwise>
					and l.status_flag in
					<foreach collection="statusFlags" item="statusFlag" index="index"
							 open="(" close=")" separator=",">
						#{statusFlag}
					</foreach>
				</otherwise>
			</choose>
		</if>
	  	order by l.create_date
        limit 300
	</select>

	<!-- 读取APP订单日志 -->
	<select id="getAppOrderLogs" resultType="OrderProcessLog">
		select l.id,l.action_comment,l.create_date,l.status_value,create_by as "createBy.id"
		from sd_orderprocesslog l
		where
		<if test="quarter != null and quarter != ''.toString()"> l.quarter = #{quarter} and </if>
			l.order_id = #{orderId}
			and l.close_flag IN (0, 2)
			AND l.status_value between 40 AND 85
			AND l.visibility_flag in (0,5,7)
		order by l.create_date desc
	</select>

	<!-- 返回变更前的订单状态 -->
	<select id="getLastOrderLog" resultType="Dict">
		select id,
		status as label,
		status_value as value
		from sd_orderprocesslog
		where
			<if test="quarter != null and quarter != ''.toString()"> quarter = #{quarter} and </if>
			order_id = #{orderId}
		order by create_date desc
		limit 30
	</select>
	<!-- 读取订单日志 -->
	<select id="getOrderLogs" resultType="OrderProcessLog">
		select l.id,l.create_date,l.action_comment,l.remarks,l.create_by as "createBy.id",'' as "createBy.name"
		from sd_orderprocesslog l
		where <if test="quarter != null and quarter != ''.toString()"> l.quarter = #{quarter} and </if>
		l.order_id = #{orderId}
		<if test="statusFlag != null">
			and l.status_flag = #{statusFlag}
		</if>
		order by l.create_date
	</select>

	<select id="checkRepeateOrder" resultType="java.lang.String">
        /*#mycat:db_type=master*/
		select order_no
		from
		<choose>
			<when test="phone2 != null and phone2 != ''.toString()">
				(
				select order_no
				from
				sd_order_condition
				where create_date between #{startDate} and #{endDate}
				and customer_id = #{customerId}
				and phone1 = #{phone1}
				union
				select order_no
				from
				sd_order_condition
				where create_date between #{startDate} and #{endDate}
				and customer_id = #{customerId}
				and phone2 = #{phone2}
				) a
				order by order_no desc
				limit 1
			</when>
			<otherwise>
				sd_order_condition
				where create_date between #{startDate} and #{endDate}
				and customer_id = #{customerId}
				and phone1 = #{phone1}
				order by order_no desc
				limit 1
			</otherwise>
		</choose>
    </select>

