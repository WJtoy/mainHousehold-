	<!-- 读取订单日志 （分页）-->
	<select id="getOrderLogsByFlagsNew" resultType="OrderProcessLog">
		select l.id,l.action,l.action_comment,l.status,l.status_flag,l.close_flag,l.create_by as "createBy.id", '' as "createBy.name",l.create_date,l.visibility_flag
		from sd_orderprocesslog l
		where
		<if test="quarter != null and quarter != ''.toString()"> l.quarter = #{quarter} and </if>
		l.order_id = #{orderId} and visibility_flag not in(2)
		<if test="closeFlag != null">
			and l.close_flag = #{closeFlag}
		</if>
		<if test="statusFlags != null and statusFlags.size > 0">
			<choose>
				<when test="statusFlags.size == 1">
					and l.status_flag =
					<foreach collection="statusFlags" item="statusFlag" index="index" open="" close="" separator="">
						#{statusFlag}
					</foreach>
				</when>
				<otherwise>
					and l.status_flag in
					<foreach collection="statusFlags" item="statusFlag" index="index" open="(" close=")" separator=",">
						#{statusFlags[${index}]}
					</foreach>
				</otherwise>
			</choose>
		</if>
		order by l.create_date desc
	</select>

    <select id="getPresetFeeWhenPlanFromMasterDB" resultType="OrderFee">
		/*#mycat:db_type=master*/
		select
			plan_travel_charge,
			customer_plan_travel_charge,
			plan_other_charge,
			customer_plan_other_charge,
			plan_travel_no,
			plan_distance
		from
			sd_order_fee
		where
			order_id = #{orderId}
			and quarter = #{quarter}
		limit 1
	</select>

	<select id="getOrderIdByCustomerId" resultType="java.lang.Long">
	    SELECT
	           order_id
	    FROM
	          sd_order_condition
	    where
	          customer_id = #{customerId}
		limit 1
	</select>

	<select id="getB2BInfoById" resultType="Order">
	    SELECT
	           data_source as 'dataSource.value',
		       parent_biz_order_id,
               shop_id as 'b2bShop.shopId'
	    FROM
	          sd_order_head
	    where
	          id = #{orderId}
		<if test="quarter !=null and quarter!=''">
			and quarter = #{quarter}
		</if>
	</select>

</mapper>
