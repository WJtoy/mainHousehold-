public class InterceptorLogEntity {
    private Log2 log2;
    private Object handler;
    private Exception ex;

    public InterceptorLogEntity(Log2 log2, Object handler, Exception ex) {
        this.log2 = log2;
        this.handler = handler;
        this.ex = ex;
    }

    public Log2 getLog() {
        return log2;
    }

    public void setLog(Log2 log2) {
        this.log2 = log2;
    }

    public Object getHandler() {
        return handler;
    }

    public void setHandler(Object handler) {
        this.handler = handler;
    }

    public Exception getEx() {
        return ex;
    }

    public void setEx(Exception ex) {
        this.ex = ex;
    }
}
