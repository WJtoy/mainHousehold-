<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wolfking.jeesite.modules.fi.dao.ServicePointDeductedDetailDao">

	<select id="getDeductedDetailAmountList" resultType="com.kkl.kklplus.entity.fi.servicepoint.ServicePointDeductedDetail">
		SELECT
			product_category_id,
			amount
		FROM
			fi_servicepoint_deducted_detail
		WHERE
			withdraw_id = #{withdrawId}
	</select>

</mapper>