<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wolfking.jeesite.modules.md.dao.TimelinessLevelDao">
    <!-- 2020-1-4 注释所有的语句
    <sql id="columns">
            a.id,
            a.name,
            a.charge_in,
            a.charge_out,
            a.sort,
            a.remarks
    </sql>
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        INSERT INTO md_timeliness_level(
			name,
			charge_in,
			charge_out,
			sort,
			create_by,
			create_date,
			remarks
		) VALUES (
			#{name},
			#{chargeIn},
			#{chargeOut},
			#{sort},
			#{createBy.id},
			#{createDate},
			#{remarks}
		)
    </insert>

    <update id="update">
        UPDATE
            md_timeliness_level
        SET
            name = #{name},
            sort = #{sort},
            charge_in = #{chargeIn},
            charge_out = #{chargeOut},
            remarks = #{remarks},
            update_by = #{updateBy.id},
            update_date = #{updateDate}
        WHERE
            id = #{id}
    </update>

    <delete id="delete">
        UPDATE
            md_timeliness_level
        SET
            del_flag = 1,
            update_by = #{updateBy.id},
            update_date = #{updateDate}
        WHERE
            id = #{id}
    </delete>

    <select id="get" resultType="TimelinessLevel">
        SELECT
               <include refid="columns"/>
        FROM
                md_timeliness_level a
        WHERE
                a.id = #{id}
          AND   a.del_flag = 0
    </select>


    <select id="findAllList" resultType="TimelinessLevel">
        SELECT
            <include refid="columns"/>
        FROM
             md_timeliness_level a
        WHERE
            a.del_flag = 0
        ORDER BY a.create_date
    </select>

    <select id="findList" resultType="TimelinessLevel">
        SELECT
            <include refid="columns"/>
        FROM
            md_timeliness_level a
        <where>
            a.del_flag = 0
        </where>
        ORDER BY a.sort,id
    </select>
    -->
</mapper>