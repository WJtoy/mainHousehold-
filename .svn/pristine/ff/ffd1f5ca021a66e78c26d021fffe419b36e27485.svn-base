<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wolfking.jeesite.ms.b2bcenter.sd.dao.B2BManualRetryDao">

    <select id="listOrders" resultType="OrderCondition">
        SELECT
              order_id,
              quarter,
              status AS "status.value",
              customer_id AS "customer.id",
              close_date
        FROM  sd_order_condition
        <where>
            AND quarter = #{quarter}
            AND customer_id IN
            <foreach collection="customerIds" open="(" separator="," close=")" item="customerId">
                #{customerId}
            </foreach>
            <![CDATA[ AND create_date >= #{beginCreateDate} AND create_date < #{endCreateDate} ]]>
        </where>
        LIMIT 200000
    </select>



</mapper>
