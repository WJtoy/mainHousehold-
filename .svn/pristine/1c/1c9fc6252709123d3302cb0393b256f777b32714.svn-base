package com.wolfking.jeesite.modules.api.entity.receipt.praise;

import com.google.common.collect.Lists;
import com.wolfking.jeesite.modules.api.entity.common.AppBaseEntity;
import com.wolfking.jeesite.modules.api.entity.common.AppDict;
import lombok.Getter;
import lombok.Setter;

import java.util.List;

public class AppPraisePicItem extends AppBaseEntity {
    /**
     * 图片位置编号
     */
    @Getter
    @Setter
    private String code = "";
    /**
     * 图片位置名称
     */
    @Getter
    @Setter
    private String name = "";
    /**
     * 图片的URL地址
     */
    @Getter
    @Setter
    private String url;

    public AppPraisePicItem() {
    }

    public AppPraisePicItem(String code, String name, String url) {
        this.code = code;
        this.name = name;
        this.url = url;
    }
}
