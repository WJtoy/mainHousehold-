package com.wolfking.jeesite.modules.api.entity.md;

import lombok.Getter;
import lombok.Setter;

/**
 * 保存网点收货地址信息
 */
public class AppSaveEngineerConsigneeAddressRequest {

    /**
     * 地址类型：1 - 网点地址、2 - 个人地址、3 - 自定义地址
     */
    @Getter
    @Setter
    private Integer addressType = 0;

    /**
     * 联系人
     */
    @Getter
    @Setter
    private String userName = "";

    /**
     * 手机号
     */
    @Getter
    @Setter
    private String userPhone = "";

    /**
     * 区域名称（全称：广东省深圳市龙华区）
     */
    @Getter
    @Setter
    private String areaName = "";

    /**
     * 详细地址（省市区街道门牌号）
     */
    @Getter
    @Setter
    private String address = "";


}
