package com.wolfking.jeesite.modules.servicepoint.ms.sd;

import com.wolfking.jeesite.modules.sd.entity.OrderItem;
import com.wolfking.jeesite.modules.sd.service.AppOrderService;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Propagation;
import org.springframework.transaction.annotation.Transactional;

import java.util.List;

@Service
@Transactional(propagation = Propagation.NOT_SUPPORTED)
public class SpAppOrderService {

    @Autowired
    private AppOrderService appOrderService;


    /**
     * 计算图片数量
     */
    public Integer getProductQty(List<OrderItem> items, Long productId) {
        return appOrderService.getProductQty(items, productId);
    }

}
