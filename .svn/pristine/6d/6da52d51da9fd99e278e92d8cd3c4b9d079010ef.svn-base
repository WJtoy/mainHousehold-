package com.wolfking.jeesite.modules.api.entity.md;

/**
 * 网点银行账号信息
 */
public class RestServicePointBankAccountInfo {

    private String bankOwner = ""; //开户人
    private String bank = "";//开户银行
    private String branch = "";//支行
    private String bankNo = "";//银行账号
    private String code = "";//验证码，5分钟内有效

    public String getBankOwner() {
        return bankOwner;
    }

    public void setBankOwner(String bankOwner) {
        this.bankOwner = bankOwner;
    }

    public String getBank() {
        return bank;
    }

    public void setBank(String bank) {
        this.bank = bank;
    }

    public String getBranch() {
        return branch;
    }

    public void setBranch(String branch) {
        this.branch = branch;
    }

    public String getBankNo() {
        return bankNo;
    }

    public void setBankNo(String bankNo) {
        this.bankNo = bankNo;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }
}
